{
  "JwtOptions": {
    "Authority": "https://securetoken.google.com/<PROJECT_ID>",
    "Issuer": "https://securetoken.google.com/<PROJECT_ID>",
    "Audience": "<PROJECT_ID>",
    "TokenCookie": "firebaseAccessToken"
  },
  "DbUsers": {
    "ConnectionString": "Server=host.docker.internal, 1433;Database=DbUsers;User Id=sa;Password=<PASSWORD>",
    "PermissionGroupsSP": "GetAllPermissionGroups",
    "UserGroupsSP": "GetAllUserGroups",
    "EmailField": "Email",
    "GroupField": "Group",
    "PermissionField": "Permission",
    "Interval": 10000 //Refresh interval in milliseconds
  },
  "LoginPage": "/login.html",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
